{
  "name": "ktane-solver",
  "version": "1.11.2",
  "description": "A library made to solve KTaNE modules",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist/",
    "prebuild": "npm run clean",
    "build": "tsc",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "transform": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cpuSonicatt/KTaNE-Bomb-Expert.git"
  },
  "keywords": [
    "ktane",
    "bomb",
    "solver"
  ],
  "author": "cpuSonicatt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cpuSonicatt/KTaNE-Bomb-Expert/issues"
  },
  "homepage": "https://github.com/cpuSonicatt/KTaNE-Bomb-Expert#readme",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "jest": "^29.0.0",
    "jest-chain": "^1.1.6",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-chain"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "/test/.*\\.(test|spec)?\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
